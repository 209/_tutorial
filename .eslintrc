{
    extends: 'airbnb-base/legacy',
    parser: 'babel-eslint',

    rules: {
        indent: ['error', 4, {
          SwitchCase: 1,
          VariableDeclarator: 1,
          outerIIFEBody: 1,
          // MemberExpression: null,
          // CallExpression: {
            // parameters: null,
          // },
          FunctionDeclaration: {
            parameters: 1,
            body: 1
          },
          FunctionExpression: {
            parameters: 1,
            body: 1
          }
        }],
    }    
}
